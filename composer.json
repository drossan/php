{
    "name": "drossan/core",
    "description": "Mini framework PHP",
    "type": "library",
    "license": "MIT",
    "version": "1",
    "authors": [{
        "name": "Team developer DROSSAN",
        "email": "core@drossan.com"
    }],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.1.3"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "mockery/mockery": "^0.9.4"
    },
    "require": {
        "aura/router": "^3.1",
        "filp/whoops": "^2.3",
        "franzl/whoops-middleware": "^1.1",
        "illuminate/database": "^5.6",
        "middlewares/aura-router": "^1.1",
        "php-di/php-di": "^6.0",
        "respect/validation": "^1.1",
        "robmorgan/phinx": "^0.10.7",
        "twig/twig": "^2.5",
        "vlucas/phpdotenv": "^2.5",
        "woohoolabs/harmony": "^6.0",
        "zendframework/zend-diactoros": "^1.8",
        "zendframework/zend-httphandlerrunner": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Drossan\\core\\": "src/"
        }
    }
}
